    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.0",
    "chance": "^1.0.16",
    "cheerio": "^1.0.0-rc.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.0",
